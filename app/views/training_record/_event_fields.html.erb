<div class="nested-fields">
    <dl>
        <dt><%= f.label :part, "対象部位" %></dt>
        <dd>
            <%= f.select :part,[["大胸筋","大胸筋"],["広背筋","広背筋"],["腹直筋","腹直筋"],["大腿四頭筋","大腿四頭筋"],["上腕三頭筋","上腕三頭筋"],["上腕二頭筋","上腕二頭筋"]],:prompt => "部位を選択" %>
        </dd>
    </dl>
    <dl>
        <dt>
            <%= f.label :name, "種目" %>
        </dt>
        <dd>
            <%= f.collection_select :name, MenuName.all, :name, :name,:prompt => "種目名を選択する" %>
        </dd>
    </dl>
    <%= f.fields_for :set_datum do |i| %>
        <%= render 'set_datum_fields', f: i %>
    <% end %>
    <div class="links">
        <%= link_to_add_association "セットを追加", f, :set_datum %>
    </div>
</div>